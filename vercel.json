{
  "version": 2,
  "builds": [
    {
      "src": "dist/an-stays-website/browser/*",
      "use": "@vercel/static" 
    },
    {
      "src": "dist/an-stays-website/server/server.mjs",
      "use": "@vercel/node" 
    }
  ],
  "routes": [
    {
      "src": "/assets/(.*)",
      "dest": "dist/an-stays-website/browser/assets/$1" 
    },
    {
      "src": "/(.*)",
      "dest": "dist/an-stays-website/server/server.mjs" 
    }
  ]
}